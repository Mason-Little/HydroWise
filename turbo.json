{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "!.vite/**"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:web": {
      "cache": false,
      "persistent": true
    },
    "dev:desktop": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "format": {},
    "check": {},
    "preview": {
      "cache": false,
      "persistent": true
    }
  }
}
